[application]

config/name="The Sun Is Off"
config/description="Submission for LOWREZJAM 2020 game jam: 2D top-down maze exploring dungeon crawling thing. As per the LOWREZJAM rules, the game must be __at most__ 64x64 pixel resolution."
run/main_scene="res://World/World.tscn"
config/icon="res://icon.png"
config/macos_native_icon="res://icon.icns"
config/windows_native_icon="res://icon.ico"

[autoload]

PlayerStats="*res://Player/PlayerStats.tscn"
Utils="*res://Utils/Utils.gd"
SaveAndLoad="*res://Utils/SaveAndLoad.gd"
ItemUtils="*res://Items/ItemUtils.gd"
Events="*res://Utils/Events.gd"

